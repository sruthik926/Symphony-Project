
  <!--form for document -->
<%= form_for @document do |d| %> <br>
  <%= d.label 'Document Name' %> <br>
  <%= d.text_field :name %> <br>

  <%= d.label 'Please Select Student' %> <br>
  <%= d.collection_select(:student_id, Student.all, :id, :full_name) %> <br>

 <%= d.label 'Document' %> <br>
 <%= d.file_field :link %> <br>

 <%= d.submit %>

<% end %>
