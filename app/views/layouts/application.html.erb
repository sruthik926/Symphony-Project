<!DOCTYPE html>
<html>
  <head>
    <title>SympProject</title>
     <!-- <link rel="stylesheet" href="styles.css"> -->
      <div class="header">
        <h1 id=centerDiv> <font size="9" > Symphony School of Music </font> </h1>
        <h4> <text_align : right>
             <% if current_user %>
             <%= current_user.full_name %>
             (<%= session[:user_type] %>)
          <%= button_to 'Logout', session_path(session), method: :delete %>
            <% end %>
        </h4>
      </div>
       <div>
       <h3> <%# if logged_in? %>
        <%# current_user.full_name %> </h3> </div>
        <%# end %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application'  %>

  </head>


  <body>
    <div class="container">
      <div id="app-container">
        <!-- <h1> Test </h1>  -->
       <%= yield %>
      </div>
    </div>
  </body>

</html>
